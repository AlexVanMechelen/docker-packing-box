#!/usr/bin/python3
# -*- coding: UTF-8 -*-
from bintropy import bintropy
from pboxtools import *


def execute(name, **kwargs):
    r = bintropy(kwargs['path'])
    return (None, "no result") if r is None else (r, None)


if __name__ == "__main__":
    run("Bintropy", exec_func=execute, normalize_output=False)

